{
  "name": "nodejs-sample",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --collectCoverage",
    "prebuild": "rimraf dist/",
    "build": "tsc",
    "start": "yarn serve",
    "seed": "ts-node src/seeds/seed.ts",
    "serve": "node dist/index.js",
    "dev": "nodemon",
    "watch-ts": "tsc -w",
    "migration:generate": "typeorm migration:create -n",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run --config ormconfig.js",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert --config ormconfig.js"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/joi": "^17.2.3",
    "@types/morgan": "^1.9.4",
    "jest": "^29.5.0",
    "nodemon": "^3.1.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@babel/core": "^7.22.5",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-winston": "^4.1.0",
    "http-status-codes": "^2.2.0",
    "joi": "^17.9.2",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17",
    "winston": "^3.8.2"
  }
}
